{
  "name": "@lcacollect/e2e-testing",
  "version": "1.0.0",
  "description": "E2E testing utilities for the LCAcollect apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "echo 'No tests!'",
    "lint": "eslint src/**/*.{tsx,ts}",
    "lint:fix": "eslint --fix src/**/*.{tsx,ts}",
    "format:fix": "prettier --write src/",
    "format": "prettier --check src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lcacollect/shared-e2e-testing-frontend.git"
  },
  "keywords": [],
  "author": "Christian Kongsgaard <chrk@arkitema.com>",
  "license": "Proprietary",
  "devDependencies": {
    "@apollo/client": "^3.7.0",
    "@graphql-tools/mock": "^8.7.6",
    "@graphql-tools/schema": "^9.0.4",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-jest": "^29.1.2",
    "cypress": "^10.10.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.30.0",
    "semver": "^7.3.8",
    "cypress-file-upload": "^5.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
